{
    "id": "arduino",
    "name": "code Arduino with PXT",
    "title": "code Arduino with PXT",
    "corepkg": "arduino",
    "cloud": {
        "workspace": false,
        "packages": true
    },
    "bundleddirs": [
        "libs/arduino"
    ],
    "blocksprj": {
        "id": "blocksprj",
        "config": {
            "name": "{0} block",
            "dependencies": {
                "arduino": "*"
            },
            "description": "",
            "files": [
                "main.blocks",
                "main.ts"
            ]
        },
        "files": {
            "main.blocks": "<xml xmlns='http://www.w3.org/1999/xhtml'>\n</xml>\n",
            "main.ts": "\n"
        }
    },
    "tsprj": {
        "id": "tsprj",
        "config": {
            "name": "{0} js",
            "dependencies": {
                "arduino": "*"
            },
            "description": "",
            "files": [
                "main.ts"
            ]
        },
        "files": {
            "main.ts": "\n"
        }
    },
    "runtime": {
        "mathBlocks": true,
        "loopsBlocks": true,
        "logicBlocks": true,
        "variablesBlocks": true
    },
    "simulator": {
        "autoRun": true,
        "aspectRatio": 1.22
    },
    "serial": {
        "manufacturerFilter": "^mbed$",
        "nameFilter": "^mbed Serial Port",
        "log": true
    },
    "appTheme": {
        "accentColor": "#00979C",
        "logoUrl": "https://sample.pxt.io/",
        "homeUrl": "https://sample.pxt.io/",
        "embedUrl": "https://sample.pxt.io/",
        "privacyUrl": "https://go.microsoft.com/fwlink/?LinkId=521839",
        "termsOfUseUrl": "https://go.microsoft.com/fwlink/?LinkID=206977",
        "privacyUrl": "https://go.microsoft.com/fwlink/?LinkId=521839",
        "termsOfUseUrl": "https://go.microsoft.com/fwlink/?LinkID=206977",
        "boardName": "Arduino Zero",
        "docMenu": [
            {
                "name": "About",
                "path": "/about"
            },
            {
                "name": "Docs",
                "path": "/doc"
            }
        ],
        "sideDoc": "docs"
    }
}
