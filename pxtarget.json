{
    "id": "sample",
    "name": "Sample PXT",
    "title": "Sample PXT Target",
    "corepkg": "sample",
    "cloud": {
        "workspace": false,
        "packages": true
    },
    "bundleddirs": [
        "libs/sample"
    ],
    "blocksprj": {
        "id": "blocksprj",
        "config": {
            "name": "{0} block",
            "dependencies": {
                "sample": "*"
            },
            "description": "",
            "files": [
                "main.blocks",
                "main.ts"
            ]
        },
        "files": {
            "main.blocks": "<xml xmlns='http://www.w3.org/1999/xhtml'>\n</xml>\n",
            "main.ts": "\n"
        }
    },
    "tsprj": {
        "id": "tsprj",
        "config": {
            "name": "{0} js",
            "dependencies": {
                "sample": "*"
            },
            "description": "",
            "files": [
                "main.ts"
            ]
        },
        "files": {
            "main.ts": "\n"
        }
    },
    "runtime": {
      "mathBlocks": true,
      "loopsBlocks": true,
      "logicBlocks": true,
      "variablesBlocks": true,
      "textBlocks": true,
      "listsBlocks": true 
    },
    "simulator": {
        "autoRun": true
    },
    "appTheme": {
        "logoUrl": "https://sample.pxt.io/",
        "homeUrl": "https://sample.pxt.io/",
        "embedUrl": "https://sample.pxt.io/",
        "privacyUrl": "https://go.microsoft.com/fwlink/?LinkId=521839",
        "termsOfUseUrl": "https://go.microsoft.com/fwlink/?LinkID=206977",
        "visualStudioCode": false,
        "docMenu": [
            {
                "name": "About",
                "path": "/about"
            },
            {
                "name": "Docs",
                "path": "/docs"
            }
        ]
    }
}
